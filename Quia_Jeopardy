javascript:(()=>{ var boxes=document.getElementsByClassName("CBQuestionGridBox");var correct=[];for(i=0;i<boxes.length;i++){var box=boxes[i];box.classList.replace("unAnswered","correct");if(box.classList.contains("correct")){correct.push(box.innerText)}}correct.sort(function(d,c){return d-c});var total=0;correct.forEach(function(a){total+=parseInt(a)});var m=Math.ceil(correct.length/2);var min=0;correct.slice(0,m).forEach(function(a){min+=parseInt(a)});var max=0;correct.slice(m,correct.length).forEach(function(a){max+=parseInt(a)});var p1=document.getElementsByClassName("playerPoints")[0];p1.innerText=prompt("Player 1 Score, between "+min+" and",max);var p2=document.getElementsByClassName("playerPoints")[1];p2.innerText=prompt("Player 2 Score should be",total-p1.innerText); })();
