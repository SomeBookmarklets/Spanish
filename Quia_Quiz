javascript:(()=>{ var list=document.getElementById("quiz-summary").getElementsByTagName("tr");for(i=1;i<list.length;i++){var innerList=list[i].getElementsByTagName("td");for(j=0;j<innerList.length;j++){inner=innerList[j].innerText;if(inner=="0"){innerList[j].innerHTML=innerList[j+1].innerHTML}else{if(inner=="incorrect"){innerList[j].innerHTML="correct"}else{if(inner.includes("Score")){innerList[j].innerHTML="<b>Score: (100%)</b>"}}}}} })();
